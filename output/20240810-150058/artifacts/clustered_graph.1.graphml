      <data key="d8">1.0</data>
      <data key="d9">"Scrooge threatens the Clerk's employment after the Clerk shows a moment of agreement with Scrooge's nephew's views on Christmas."</data>
      <data key="d10">080d8e696ff38c653ca90fa086415e74</data>
      <data key="d11">1dbc51475cb04dafa4a8833a8378635e</data>
      <data key="d12">61</data>
      <data key="d13">1</data>
    </edge>
    <edge source="&quot;SCROOGE&quot;" target="&quot;PORTLY GENTLEMEN&quot;">
      <data key="d8">1.0</data>
